{
    "manifest_version": 3,
    "name": "Streamrip Downloader",
    "version": "1.0",
    "description": "Send URLs to a Linux server running streamrip",
    "permissions": [
      "contextMenus",
      "notifications",
      "storage"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "options_page": "options.html"
  }